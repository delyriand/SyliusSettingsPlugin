<div class="alert mb-0" role="alert">
    {% if setting('app.default', 'enabled') %}
        <div class="h5">{{ setting('app.default', 'demo_title') }}</div>
        <div>{{ setting('app.default', 'demo_message') }}</div>
    {% else %}
        <div>{{ 'Message is disabled'|trans }}</div>
    {% endif %}
</div>
